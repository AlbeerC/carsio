    },
    "price": {
      "type": "biginteger",
      "required": true,
      "min": "1"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "transmission": {
      "type": "string",
      "required": true,
      "maxLength": 15
    },
    "km": {
      "type": "biginteger"
    },
    "version": {
      "type": "text"
    },
    "images": {
      "type": "json"
    }
